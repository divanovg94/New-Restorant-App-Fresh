<div class="container">   
<div class="row" style="margin-top:60px">
    <div class="col-xs-12 col-sm-8 col-md-6 col-sm-offset-2 col-md-offset-3">
  
<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>
     <fieldset>
        <h2>Sign Up</h2>
        <hr class="colorgraph">
        <div class="form-group">

    <%= f.label :first_name  %><br />
    <%= f.text_field :first_name ,id: 'email',class: "form-control input-lg"  %>
  </div>
    <div class="form-group">
    <%= f.label :last_name %><br />
    <%= f.text_field :last_name ,id: 'email',class: "form-control input-lg"  %>
  </div>
  <div class="form-group">
    <%= f.label :email  %><br />
    <%= f.email_field :email, autofocus: true , id: 'email',class: "form-control input-lg"  %>
  </div>

  <div class="form-group">
    <%= f.label :password  %>
    <% if @minimum_password_length %>
    <em>(<%= @minimum_password_length %> characters minimum)</em>
    <% end %><br />
    <%= f.password_field :password, autocomplete: "off"  , id: 'password' , class: "form-control input-lg"  %>
  </div>

 <div class="form-group">
    <%= f.label :password_confirmation  %><br />
    <%= f.password_field :password_confirmation, autocomplete: "off" , id: 'email_id',id: 'password',class: "form-control input-lg"  %>
  </div>
  <hr class="colorgraph">
        <div class="row">
          <div class="col-xs-6 col-sm-6 col-md-6">
            <%= f.submit "Sign up" , { :class => 'btn btn-lg btn-success btn-block'}  %>
          </div>
        <div class="col-xs-6 col-sm-6 col-md-6">
            <a href="/users/sign_in" class="btn btn-lg btn-primary btn-block">Back</a>
          </div>
       </div>
        </div>
      </fieldset>
    <% end %>
      </div>
</div>
</div>  
